{
  "extends": "@flippo/tsconfig",
  "compilerOptions": {
    "jsx": "react-jsx",
    "lib": ["DOM", "DOM.Iterable", "ESNext"],

    "baseUrl": ".",
    "types": ["node", "react", "react-dom"],
    "allowJs": true,
    "noEmit": true
  },
  "include": ["src/**/*.ts", "src/**/*.js", "src/**/*.tsx", "vite.config.ts", "src/scripts/constants.js", "src/scripts/renameSvg.js"],
  "exclude": ["node_modules", "dist"]
}
