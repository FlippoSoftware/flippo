{
  "name": "@flippo/internal-test-utils",
  "version": "1.0.0",
  "description": "Internal testing utilities for Flippo headless UI components",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest",
    "test:ui": "vitest --ui"
  },
  "keywords": [
    "testing",
    "test-utils",
    "headless-ui",
    "react",
    "flippo",
    "internal"
  ],
  "author": "@BlackPoretsky",
  "license": "ISC",
  "private": true,
  "packageManager": "pnpm@10.14.0",
  "dependencies": {
    "@testing-library/jest-dom": "catalog:",
    "@testing-library/react": "catalog:",
    "@testing-library/user-event": "catalog:",
    "@vitest/ui": "catalog:",
    "jsdom": "catalog:",
    "vitest": "catalog:"
  },
  "devDependencies": {
    "@types/node": "catalog:",
    "@types/react": "catalog:",
    "@types/react-dom": "catalog:",
    "tsup": "catalog:",
    "typescript": "catalog:"
  },
  "peerDependencies": {
    "react": "catalog:",
    "react-dom": "catalog:",
    "vitest": "catalog:"
  }
}
